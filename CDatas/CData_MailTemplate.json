    },
    "850013": {
      "m_bError": null,
      "m_MailTemplateID": "850013",
      "m_ClientCanSend": null,
      "m_Sticky": null,
      "m_mFrom": "\"Agatha\"",
      "m_mTitle": "\"Cross-Dimensional Study: Join Us!\"",
      "m_mContent": "\"To whom it may concern:\n\n My name is Agnes, an apprentice Cartographer destined to become the greatest in Minexus!\n Soon, I shall embark upon a cross-dimensional study. Your participation would add a splendid chapter to this grand voyage.\n Compensation and job responsibilities may be discussed upon meeting.\n As a gesture of goodwill, I've enclosed a small gift with this letter. Those interested should come on the night of the full moon to...\n (The handwriting becomes illegible here. You suspect this might be some childish prank.)\"",
      "m_DelTime": "\"\"",
      "m_DelDurTime": null,
      "m_Attachment": "[[500061,1],[101,100]]",
      "m_SaveAfterRcvAttach": null,
